<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
        <tbody>
          <% @votings.each do |voting| %>
          <tr>
                        <td><%= voting.votable_id %></td>
                        <td><%= voting.votable_type %></td>
                        <td><%= voting.deadline %></td>
                        <td><%= voting.voting_type %></td>
                        <td><%= link_to t(:show), voting %></td>
            <td><%= link_to t(:edit), edit_voting_path(voting) %></td>
            <td><%= link_to t(:destroy), voting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
