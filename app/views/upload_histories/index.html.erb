<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
        <tbody>
          <% @upload_histories.each do |upload_history| %>
          <tr>
                        <td><%= upload_history.uploadable_type %></td>
                        <td><%= upload_history.uploadable_id %></td>
                        <td><%= upload_history.token %></td>
                        <td><%= upload_history.attachment_type %></td>
                        <td><%= upload_history.revision_number %></td>
                        <td><%= upload_history.user_id %></td>
                        <td><%= upload_history.workflow_transition_id %></td>
                        <td><%= upload_history.speaking_id %></td>
                        <td><%= link_to t(:show), upload_history %></td>
            <td><%= link_to t(:edit), edit_upload_history_path(upload_history) %></td>
            <td><%= link_to t(:destroy), upload_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
