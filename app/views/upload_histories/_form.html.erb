<%= form_for(@upload_history, html: {class: 'card'}) do |f| %>
<div class="card-header">
  <h3 class="card-title"><%=t :new_upload_history %></h3>
</div>
<% if @upload_history.errors.any? %>
<div id="error_explanation">
  <h2><%= pluralize(@upload_history.errors.count, "error") %> prohibited this upload_history from being saved:</h2>

  <ul>
    <% @upload_history.errors.full_messages.each do |message| %>
    <li><%= message %></li>
    <% end %>
  </ul>
</div>
<% end %>

<div class="card-body">
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :uploadable_type, class: "form-label" %>
      <%= f.text_field :uploadable_type , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :uploadable_id, class: "form-label" %>
      <%= f.number_field :uploadable_id , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :token, class: "form-label" %>
      <%= f.text_field :token , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :attachment_type, class: "form-label" %>
      <%= f.text_field :attachment_type , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :revision_number, class: "form-label" %>
      <%= f.text_field :revision_number , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :user_id, class: "form-label" %>
      <%= f.number_field :user_id , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :workflow_transition_id, class: "form-label" %>
      <%= f.number_field :workflow_transition_id , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :speaking_id, class: "form-label" %>
      <%= f.number_field :speaking_id , class: "form-control" %>
          </div>
  </div>
</div>
<div class="card-footer text-right">
  <button type="submit" class="btn btn-primary"><%=t :submit%></button>
</div>
<% end %>
