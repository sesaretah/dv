<%= form_for(@formating_history, html: {class: 'card'}) do |f| %>
<div class="card-header">
  <h3 class="card-title"><%=t :new_formating_history %></h3>
</div>
<% if @formating_history.errors.any? %>
<div id="error_explanation">
  <h2><%= pluralize(@formating_history.errors.count, "error") %> prohibited this formating_history from being saved:</h2>

  <ul>
    <% @formating_history.errors.full_messages.each do |message| %>
    <li><%= message %></li>
    <% end %>
  </ul>
</div>
<% end %>

<div class="card-body">
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :article_format_id, class: "form-label" %>
      <%= f.number_field :article_format_id , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :article_id, class: "form-label" %>
      <%= f.number_field :article_id , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :user_id, class: "form-label" %>
      <%= f.number_field :user_id , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :revision_number, class: "form-label" %>
      <%= f.text_field :revision_number , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :workflow_state_id, class: "form-label" %>
      <%= f.number_field :workflow_state_id , class: "form-control" %>
          </div>
  </div>
</div>
<div class="row">
  <div class="col-md-5">
    <div class="form-group">
          <%= f.label :formating_id, class: "form-label" %>
      <%= f.number_field :formating_id , class: "form-control" %>
          </div>
  </div>
</div>
<div class="card-footer text-right">
  <button type="submit" class="btn btn-primary"><%=t :submit%></button>
</div>
<% end %>
