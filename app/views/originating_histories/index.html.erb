<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
        <tbody>
          <% @originating_histories.each do |originating_history| %>
          <tr>
                        <td><%= originating_history.article_id %></td>
                        <td><%= originating_history.article_source_id %></td>
                        <td><%= originating_history.originating_id %></td>
                        <td><%= originating_history.revision_number %></td>
                        <td><%= originating_history.user_id %></td>
                        <td><%= originating_history.workflow_transition_id %></td>
                        <td><%= link_to t(:show), originating_history %></td>
            <td><%= link_to t(:edit), edit_originating_history_path(originating_history) %></td>
            <td><%= link_to t(:destroy), originating_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
