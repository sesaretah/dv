<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
        <tbody>
          <% @datings.each do |dating| %>
          <tr>
                        <td><%= dating.article_event_id %></td>
                        <td><%= dating.article_id %></td>
                        <td><%= dating.event_date %></td>
                        <td><%= link_to t(:show), dating %></td>
            <td><%= link_to t(:edit), edit_dating_path(dating) %></td>
            <td><%= link_to t(:destroy), dating, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
