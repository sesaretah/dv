<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
        <tbody>
          <% @votes.each do |vote| %>
          <tr>
                        <td><%= vote.user_id %></td>
                        <td><%= vote.voting_id %></td>
                        <td><%= vote.outcome %></td>
                        <td><%= link_to t(:show), vote %></td>
            <td><%= link_to t(:edit), edit_vote_path(vote) %></td>
            <td><%= link_to t(:destroy), vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
