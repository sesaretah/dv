<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
        <tbody>
          <% @contribution_histories.each do |contribution_history| %>
          <tr>
                        <td><%= contribution_history.article_id %></td>
                        <td><%= contribution_history.role_id %></td>
                        <td><%= contribution_history.duty_id %></td>
                        <td><%= contribution_history.profile_id %></td>
                        <td><%= contribution_history.revision_number %></td>
                        <td><%= contribution_history.user_id %></td>
                        <td><%= contribution_history.workflow_transition_id %></td>
                        <td><%= contribution_history.contribution_id %></td>
                        <td><%= link_to t(:show), contribution_history %></td>
            <td><%= link_to t(:edit), edit_contribution_history_path(contribution_history) %></td>
            <td><%= link_to t(:destroy), contribution_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
