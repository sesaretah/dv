<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
        <tbody>
          <% @kinships.each do |kinship| %>
          <tr>
                        <td><%= kinship.user_id %></td>
                        <td><%= kinship.kin_id %></td>
                        <td><%= link_to t(:show), kinship %></td>
            <td><%= link_to t(:edit), edit_kinship_path(kinship) %></td>
            <td><%= link_to t(:destroy), kinship, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
