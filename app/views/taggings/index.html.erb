<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
        <tbody>
          <% @taggings.each do |tagging| %>
          <tr>
                        <td><%= tagging.taggable_id %></td>
                        <td><%= tagging.taggable_type %></td>
                        <td><%= tagging.target_id %></td>
                        <td><%= tagging.target_type %></td>
                        <td><%= link_to t(:show), tagging %></td>
            <td><%= link_to t(:edit), edit_tagging_path(tagging) %></td>
            <td><%= link_to t(:destroy), tagging, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
