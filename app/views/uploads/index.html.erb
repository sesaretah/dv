<div class="row row-cards">
  <div class="col-lg-3">
    <div class="row">
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
      <div class="col-md-6 col-lg-12">
        <div class="card">
          <div class="card-body">

          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="col-lg-9">
    <div class="card">
      <table class="table card-table table-vcenter">
        <tbody>
          <% @uploads.each do |upload| %>
          <tr>
                        <td><%= upload.uploadable_type %></td>
                        <td><%= upload.uploadable_id %></td>
                        <td><%= upload.token %></td>
                        <td><%= link_to t(:show), upload %></td>
            <td><%= link_to t(:edit), edit_upload_path(upload) %></td>
            <td><%= link_to t(:destroy), upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
